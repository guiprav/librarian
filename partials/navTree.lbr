<ul class="lbrNavTree">
    {{#recurse hash.navTree}}
        <li class="lbrNavTree_node">
            <a href="{{either href '#toggle'}}">
                {{@key}}
            </a>

            {{#if children}}
                <ul>
                    {{recurse children}}
                </ul>
            {{/if}}
        </li>
    {{/recurse}}
</ul>
